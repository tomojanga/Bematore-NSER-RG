# NSER & RG Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000/api/v1
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws

# Authentication
NEXT_PUBLIC_JWT_SECRET=your-jwt-secret-here
NEXT_PUBLIC_REFRESH_TOKEN_SECRET=your-refresh-token-secret-here

# External Services
NEXT_PUBLIC_MICROSOFT_CLARITY_ID=your-clarity-id
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your-ga-id

# Development
NEXT_PUBLIC_DEBUG=true
NEXT_PUBLIC_LOG_LEVEL=debug

# Production URLs (when deployed)
# NEXT_PUBLIC_API_URL=https://api.nser.go.ke/api/v1
# NEXT_PUBLIC_WS_URL=wss://api.nser.go.ke/ws